{
	"name": "nestjs-template",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"start": "nest start",
		"test": "vitest --coverage",
		"format": "prettier --write \"{src,test}/**/*.ts\"",
		"lint": "npx eslint \"{src,test}/**/*.ts\" --fix",
		"build": "nest build",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"infrastructure:up": "docker compose -f infrastructure.yaml up -d",
		"infrastructure:down": "docker compose -f infrastructure.yaml down",
		"test:create": "ts-node scripts/create-integration-tests.ts"
	},
	"dependencies": {
		"@nestjs/bullmq": "^11.0.3",
		"@nestjs/common": "11.1.6",
		"@nestjs/config": "^4.0.2",
		"@nestjs/core": "11.1.6",
		"@nestjs/event-emitter": "^3.0.1",
		"@nestjs/platform-express": "11.1.6",
		"@nestjs/schedule": "^6.0.0",
		"@nestjs/swagger": "^11.2.0",
		"bullmq": "^5.58.2",
		"class-transformer": "0.5.1",
		"class-validator": "0.14.2",
		"dataloader": "^2.2.3",
		"reflect-metadata": "0.2.2",
		"rxjs": "7.8.2",
		"zod": "^4.1.5"
	},
	"devDependencies": {
		"@eslint/js": "9.34.0",
		"@nestjs/cli": "11.0.10",
		"@nestjs/schematics": "11.0.7",
		"@nestjs/testing": "11.1.6",
		"@swc/core": "^1.13.5",
		"@types/express": "5.0.3",
		"@types/node": "22.13.10",
		"@vitest/coverage-v8": "^3.2.4",
		"@vitest/ui": "3.2.4",
		"axios": "1.11.0",
		"eslint": "9.34.0",
		"prettier": "3.6.2",
		"source-map-support": "0.5.21",
		"ts-loader": "9.5.4",
		"ts-node": "10.9.2",
		"tsconfig-paths": "4.2.0",
		"typescript": "5.9.2",
		"typescript-eslint": "8.41.0",
		"unplugin-swc": "^1.5.7",
		"vitest": "^3.2.4"
	}
}
